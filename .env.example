# rdgen-ngx Configuration
# Copy this file to .env and fill in your values

# Django secret key (generate a random string)
SECRET_KEY=your-secret-key-here

# GitHub configuration
GHUSER=your-github-username
GHBEARER=ghp_your_github_personal_access_token
REPONAME=rustdesk
GHBRANCH=master

# Build encryption password (generate a random string)
ZIP_PASSWORD=your-zip-password-here

# Public URL where rdgen is accessible
GENURL=https://rdgen.yourdomain.com
PROTOCOL=https

# Email notifications (optional)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USE_TLS=true
EMAIL_HOST_USER=your-email@gmail.com
EMAIL_HOST_PASSWORD=your-app-password
DEFAULT_FROM_EMAIL=noreply@yourdomain.com
